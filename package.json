      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": -1000
      }
    }
  },
  "dependencies": {
    "codacy": "^1.0.1-1",
    "codacy-coverage": "^3.0.0",
    "exp": "^54.0.0",
    "expo": "^27.0.1",
    "react": "16.3.1",
    "react-native": "~0.55.2"
  }
